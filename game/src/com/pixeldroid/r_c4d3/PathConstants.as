
package com.pixeldroid.r_c4d3 {

	/**
	* The PathConstants utility class is an all-static class
	* containing constants for working with paths in the R-C4D3 system.
	* You do not create instances of PathConstants&mdash;
	* instead you refer to the static constants as 
	* <code>PathConstants.RETURN_TO_MENU</code>.
	*/
	public class PathConstants {
	
		/**
		* Defines the relative path from a game ROM to the menu front-end.
		*/
		public static const RETURN_TO_MENU:String = "../../menu/menu.html";
		
	}
}
